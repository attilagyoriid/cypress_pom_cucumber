{
  "json": {
    "enabled": false,
    "output": "jsonlogs/log.json",
    "formatter": "cucumber-json-formatter.exe"
  },
  "messages": {
    "enabled": false,
    "output": "jsonlogs/messages.ndjson"
  },
  "html": {
    "enabled": false
  },
  "stepDefinitions": [
    "[filepath]/**/*.ts",
    "[filepath].ts",
    "cypress/e2e/step_definitions/*.ts",
    "[filepath]\\***.ts",
    "[filepath].ts",
    "cypress\\e2e\\step_definitions\\*.ts"
  ]
}
